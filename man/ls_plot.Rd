\name{ls_plot}
\alias{ls_plot}

\title{
  Launch Speed Plot
}
\description{
  Constructs launch speed graph
}
\usage{
  ls_plot(df, title = "Launch Velocity")
}
\arguments{
  \item{df}{
    data frame or list containing Statcast data
  }
  \item{title}{
    title for the graph
  }
}
\value{
  Constructs a tile plot of the launch speeds
}
\author{
  Jim Albert
}


