\name{called_strike_plot}
\alias{called_strike_plot}

\title{
  Called Strike Plot
}
\description{
  Constructs called strike plot for a specific player
}
\usage{
  called_strike_plot(df, title = "Probability of Called Strike", NCOL = 2)
}
\arguments{
  \item{df}{
    data frame or list containing Statcast data
  }
  \item{title}{
    title for the graph
  }
  \item{NCOL}{
    number of columns in multipanel display
  }
}
\value{
  Constructs a tile plot of the called strike probability
}
\author{
  Jim Albert
}


