\name{setup_all}
\alias{setup_all}

\title{
  Sets up Statcast data to define new variables
}
\description{
  Sets up Statcast data to define Swing, Count and Pitch Type
}
\usage{
  setup_all(sc)
}
\arguments{
  \item{sc}{
    Statcast data frame
  }
}
\value{
  Statcast data frame for all pitches with new variables Swing, Count, Count_Type and Pitch_Type
}
\author{
  Jim Albert
}


