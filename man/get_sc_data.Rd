\name{get_sc_data}
\alias{get_sc_data}

\title{
  Scrapes Statcast data
}
\description{
  Scrapes Statcast data for a specific hitter
}
\usage{
  get_sc_data(id, season = 2018)
}
\arguments{
  \item{id}{
    MLBAM player id
  }
  \item{season}{
    Season
  }
}
\value{
  Statcast data for player for particular season
}
\author{
  Jim Albert
}


