\name{hit_contour}
\alias{hit_contour}

\title{
  Hit Contour Plot
}
\description{
  Constructs hit contour plot for a specific player
}
\usage{
  hit_contour(data, L, title)
}
\arguments{
  \item{data}{
    data frame containing Statcast data
  }
  \item{L}{
    values for contour lines
  }
  \item{title}{
    title of the graph
  }
}
\value{
  Constructs a contour plot of the hit probability
}
\author{
  Jim Albert
}


