\name{swing_contour}
\alias{swing_contour}

\title{
  Swing Contour Plot
}
\description{
  Constructs swing contour plot for a specific player
}
\usage{
  swing_contour(data, L, title)
}
\arguments{
  \item{data}{
    data frame containing Statcast data
  }
  \item{L}{
    values for contour lines
  }
  \item{title}{
    title of the graph
  }
}
\value{
  Constructs a contour plot of the swing probability
}
\author{
  Jim Albert
}


