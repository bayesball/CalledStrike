\name{swing_contour}
\alias{swing_contour}

\title{
  Swing Contour Plot
}
\description{
  Constructs swing contour plot
}
\usage{
  swing_contour(df, L = seq(0, 1, by = 0.1), title = "Probability of Swing")
}
\arguments{
  \item{df}{
    data frame or list containing Statcast data
  }
  \item{L}{
    values for contour lines
  }
  \item{title}{
    title of the graph
  }
}
\value{
  Constructs a contour plot of the swing probability
}
\author{
  Jim Albert
}


