\name{contact_swing_plot}
\alias{contact_swing_plot}

\title{
  Contact on Swings Plot
}
\description{
  Constructs contact on swung balls graph
}
\usage{
  contact_swing_plot(df, title = "Probability of Contact", NCOL = 2)
}
\arguments{
  \item{df}{
    data frame or list containing Statcast data
  }
  \item{title}{
    title for the graph
  }
  \item{NCOL}{
    number of columns in multipanel display
  }
}
\value{
  Constructs a tile plot of the contact on swung balls probability
}
\author{
  Jim Albert
}


