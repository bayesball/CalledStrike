\name{la_plot}
\alias{la_plot}

\title{
  Launch Angle Plot
}
\description{
  Constructs launch angle graph
}
\usage{
  la_plot(df, title = "Launch Angle", NCOL = 2)
}
\arguments{
  \item{df}{
    data frame or list containing Statcast data
  }
  \item{title}{
    title for the graph
  }
  \item{NCOL}{
    number of columns in multipanel display
  }
}
\value{
  Constructs a tile plot of the launch angles
}
\author{
  Jim Albert
}


