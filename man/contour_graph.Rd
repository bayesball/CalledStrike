\name{contour_graph}
\alias{contour_graph}

\title{
  Contour Graph
}
\description{
  Constructs filled contour graph
}
\usage{
  contour_graph(df, L, title)
}
\arguments{
  \item{df}{
    data frame containing Statcast and prediction variables
  }
  \item{L}{
    vector of values for contour lines
  }
  \item{title}{
    title for the graph
  }
}
\value{
  Constructs a filled contour graph
}
\author{
  Jim Albert
}


